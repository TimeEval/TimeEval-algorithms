{
    "title": "HBOS",
    "description": "Implementation of https://citeseerx.ist.psu.edu/viewdoc/citations;jsessionid=2B4E3FB2BB07448253B4D45C3DAC2E95?doi=10.1.1.401.5686.",
    "inputDimensionality": "multivariate",
    "version": "0.1",
    "authors": "Markus Goldstein, Andreas Dengel",
    "language": "Python",
    "type": "Detector",
    "mainFile": "algorithm.py",
    "learningType": "unsupervised",
    "executionStep": {
        "parameters": [
            {
                "name": "n_bins",
                "type": "int",
                "defaultValue": 10,
                "optional": "true",
                "description": "The number of bins."
            },
            {
                "name": "alpha",
                "type": "float",
                "defaultValue": 0.1,
                "optional": "true",
                "description": "Regulizing alpha to prevent overflows."
            },
            {
                "name": "bin_tol",
                "type": "float",
                "defaultValue": 0.5,
                "optional": "true",
                "description": "Parameter to decide the flexibility while dealing with the samples falling outside the bins."
            },
            {
                "name": "random_state",
                "type": "int",
                "defaultValue": 42,
                "optional": "true",
                "description": "Seed for random number generation."
            }
        ],
        "modelInput": "none"
    }
}
